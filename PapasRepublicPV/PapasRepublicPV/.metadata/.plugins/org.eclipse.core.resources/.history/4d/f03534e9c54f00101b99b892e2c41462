package clases;

public class Usuario {
    private int codigo;
    private String usuario;
    private String contrasenia;
    private String nombre;
    private String apPaterno;
    private String apMaterno;
    private int permisoCreate;
    private int permisoRead;
    private int permisoUpdate;
    private int permisoDelete;
    
	public Usuario() {

	}

	public int getCodigo() {
		return codigo;
	}

	public void setCodigo(int codigo) {
		this.codigo = codigo;
	}

	public String getUsuario() {
		return usuario;
	}

	public void setUsuario(String usuario) {
		this.usuario = usuario;
	}

	public String getContrasenia() {
		return contrasenia;
	}

	public void setContrasenia(String contrasenia) {
		this.contrasenia = contrasenia;
	}

	public String getApPaterno() {
		return apPaterno;
	}

	public void setApPaterno(String apPaterno) {
		this.apPaterno = apPaterno;
	}

	public String getApMaterno() {
		return apMaterno;
	}

	public void setApMaterno(String apMaterno) {
		this.apMaterno = apMaterno;
	}

	public int getPermisoCreate() {
		return permisoCreate;
	}

	public void setPermisoCreate(int permisoCreate) {
		this.permisoCreate = permisoCreate;
	}

	public int getPermisoRead() {
		return permisoRead;
	}

	public void setPermisoRead(int permisoRead) {
		this.permisoRead = permisoRead;
	}

	public int getPermisoUpdate() {
		return permisoUpdate;
	}

	public void setPermisoUpdate(int permisoUpdate) {
		this.permisoUpdate = permisoUpdate;
	}

	public int getPermisoDelete() {
		return permisoDelete;
	}

	public void setPermisoDelete(int permisoDelete) {
		this.permisoDelete = permisoDelete;
	}
	
    
}